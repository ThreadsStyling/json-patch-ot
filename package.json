{
  "name": "@threads/json-patch-ot",
  "version": "1.0.0",
  "description": "Library to reconcile JSON patch changes using Operational Transformation",
  "main": "lib/index.js",
  "scripts": {
    "tslint": "tslint './src/**/*.ts' -t verbose",
    "lint": "yarn tslint",
    "build": "tsc",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "prettier:diff": "prettier --list-different 'src/**/*.{ts,js,json}'",
    "precommit": "pretty-quick --staged && yarn lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThreadsStyling/json-patch-ot.git"
  },
  "keywords": [
    "json",
    "patch",
    "ot",
    "operational",
    "transformation"
  ],
  "author": "jasoniangreen@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThreadsStyling/json-patch-ot/issues"
  },
  "homepage": "https://github.com/ThreadsStyling/json-patch-ot#readme",
  "devDependencies": {
    "@threads/tslint": "^3.1.0",
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "husky": "^0.14.3",
    "jest": "^23.3.0",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [],
    "testRegex": ".*/__tests__/.*\\.(test|spec)\\.ts$"
  }
}
